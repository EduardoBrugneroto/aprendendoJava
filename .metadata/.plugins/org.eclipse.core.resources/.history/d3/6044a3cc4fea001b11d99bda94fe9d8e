
public class TestaVariaveis {

	public static void main(String[] args) {
		System.out.println("ola novo teste");
		
		int idade; 
		idade = 15;
		
		System.out.println(idade);
		
		idade = 30 + 2;
		System.out.println(idade);
		idade = (7 * 10) + 2; 
		System.out.println(idade);
		
		System.out.println("A idade é" + idade);
	}
}
